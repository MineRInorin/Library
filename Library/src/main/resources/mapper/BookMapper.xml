<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lib.demo.mapper.BookMapper">
<insert id="insertBook" parameterType="com.lib.demo.bean.Book">
insert into book(name,id,author,total,type) 
values(#{name},#{id},#{author},#{total},#{type})
</insert>
</mapper>